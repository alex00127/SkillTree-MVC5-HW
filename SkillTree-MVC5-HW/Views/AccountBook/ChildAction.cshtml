@model SkillTree_MVC5_HW.Models.ViewModels.AccountBookViewModels
@{
    @(Layout = "")
}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            <tr>
                <td>@Model.Id</td>
                <td>@Model.Category</td>
                <td>@Model.Date</td>
                <td>@Model.Amount</td>
            </tr>
        </table>
    </div>
</div>


